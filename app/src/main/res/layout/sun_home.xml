<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/latitude_title"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".zimeng.SunHome">

  <androidx.appcompat.widget.Toolbar
    android:id="@+id/toolbar"
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:background="?attr/colorPrimary"
    android:backgroundTint="#F76C5F"
    android:elevation="4dp"
    android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

  <TextView
    android:id="@+id/sunrise_label"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="88dp"
    android:layout_marginTop="40dp"
    android:fontFamily="@font/roboto_medium"
    android:text="SUNRISE"
    android:textColor="#FB7954"
    android:textSize="18sp"
    app:layout_constraintEnd_toStartOf="@+id/sunset_label"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <TextView
    android:id="@+id/sunset_label"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="36dp"
    android:layout_marginEnd="84dp"
    android:fontFamily="@font/roboto_medium"
    android:text="SUNSET"
    android:textColor="#FB7954"
    android:textSize="18sp"
    android:typeface="normal"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <TextView
    android:id="@+id/sunrise_time"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp"
    android:fontFamily="@font/roboto_medium"
    android:text="sunrise time"
    android:textColor="#E35C58"
    android:textSize="26sp"
    app:layout_constraintEnd_toStartOf="@+id/sunset_time"
    app:layout_constraintHorizontal_bias="0.708"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/sunrise_label" />

  <ImageView
    android:id="@+id/imageView"
    android:layout_width="34dp"
    android:layout_height="33dp"
    android:layout_marginStart="32dp"
    android:layout_marginTop="28dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/toolbar"
    app:srcCompat="@drawable/sun_home_date" />

  <TextView
    android:id="@+id/sunset_time"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="36dp"
    android:layout_marginEnd="20dp"
    android:fontFamily="@font/roboto_medium"
    android:text="sunset time"
    android:textColor="#E35C58"
    android:textSize="26sp"
    app:layout_constraintBottom_toTopOf="@+id/btn_sunhome_search"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/sunset_label"
    app:layout_constraintVertical_bias="0.0" />

  <ImageView
    android:id="@+id/imageView2"
    android:layout_width="424dp"
    android:layout_height="218dp"
    android:src="@drawable/sun_home_img"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.444" />

  <TextView
    android:id="@+id/textView7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="40dp"
    android:layout_marginTop="36dp"
    android:fontFamily="@font/roboto"
    android:text="Lat:"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@+id/imageView2"
    app:layout_constraintEnd_toStartOf="@+id/latitude"
    app:layout_constraintHorizontal_bias="0.568"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/imageView"
    app:layout_constraintVertical_bias="0.075" />

  <TextView
    android:id="@+id/longtitude_title"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="60dp"
    android:layout_marginTop="36dp"
    android:fontFamily="@font/roboto"
    android:text="Lng:"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@+id/imageView2"
    app:layout_constraintEnd_toStartOf="@+id/longtitude"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toEndOf="@+id/latitude"
    app:layout_constraintTop_toBottomOf="@+id/btn_sunhome_addfav"
    app:layout_constraintVertical_bias="0.121" />

  <TextView
    android:id="@+id/latitude"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="44dp"
    android:fontFamily="@font/roboto_medium"
    android:text="latitude"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@+id/imageView2"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/date"
    app:layout_constraintVertical_bias="0.113" />

  <TextView
    android:id="@+id/longtitude"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="16dp"
    android:fontFamily="@font/roboto_medium"
    android:text="longtitude"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@+id/imageView2"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/btn_sunhome_addfav" />

  <EditText
    android:id="@+id/input_lat"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="32dp"
    android:layout_marginTop="8dp"
    android:drawableStart="@drawable/sun_home_search"
    android:ems="10"
    android:fontFamily="@font/roboto"
    android:hint="  Latitude"
    android:inputType="numberDecimal|numberSigned"
    android:textColor="#676767"
    android:textSize="16sp"
    app:layout_constraintEnd_toStartOf="@+id/btn_sunhome_search"
    app:layout_constraintHorizontal_bias="0.025"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/sunrise_time" />

  <EditText
    android:id="@+id/input_long"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="32dp"
    android:layout_marginBottom="16dp"
    android:drawableStart="@drawable/sun_home_search"
    android:ems="10"
    android:fontFamily="@font/roboto"
    android:hint="  Longitude"
    android:inputType="numberDecimal|numberSigned"
    android:textColor="#676767"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/input_lat" />

  <Button
    android:id="@+id/btn_sunhome_search"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="12dp"
    android:layout_marginBottom="12dp"
    android:backgroundTint="#E4575D"
    android:fontFamily="@font/roboto"
    android:text="Search"
    android:textSize="16sp"
    app:layout_constraintBottom_toTopOf="@+id/btn_sunhome_fav"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.483"
    app:layout_constraintStart_toEndOf="@+id/input_long" />

  <Button
    android:id="@+id/btn_sunhome_addfav"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="12dp"
    android:layout_marginEnd="28dp"
    android:backgroundTint="#FDD174"
    android:fontFamily="@font/roboto"
    android:text="Add"
    android:textColor="#E25B58"
    android:textSize="16sp"
    app:icon="@drawable/sun_home_add"
    app:iconPadding="10dp"
    app:iconTint="#E25B58"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/toolbar" />

  <TextView
    android:id="@+id/date"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="28dp"
    android:fontFamily="@font/roboto_bold"
    android:text="TextView"
    android:textSize="20sp"
    app:layout_constraintEnd_toStartOf="@+id/btn_sunhome_addfav"
    app:layout_constraintHorizontal_bias="0.155"
    app:layout_constraintStart_toEndOf="@+id/imageView"
    app:layout_constraintTop_toBottomOf="@+id/toolbar" />

  <Button
    android:id="@+id/btn_sunhome_fav"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="32dp"
    android:backgroundTint="#E4575D"
    android:fontFamily="@font/roboto"
    android:text="Favourites"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.464"
    app:layout_constraintStart_toEndOf="@+id/input_long" />

</androidx.constraintlayout.widget.ConstraintLayout>